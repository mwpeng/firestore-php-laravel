{
    "name": "mwpeng/firestore-php-laravel",
    "description": "Laravel wrapper of morrislaptop/firestore-php",
    "keywords": ["firestore", "laravel"],
    "type": "laravel-pakcage",
    "license": "MIT",
    "authors": [
        {
            "name": "Mingwei Peng",
            "email": "mingw.peng@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.7",
        "morrislaptop/firestore-php": "^2.2"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Mwpeng\\FirestorePhpLaravel\\ServiceProvider"
            ],
            "aliases": {
                "Firestore": "Mwpeng\\FirestorePhpLaravel\\Facade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Mwpeng\\FirestorePhpLaravel\\": "src/"
        },
        "files": [
        ]
    },
    "require-dev": {
        "laravel/framework": "^5.7"
    }
}
